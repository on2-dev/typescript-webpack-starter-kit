    "concurrently": "^5.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "nodemon": "^2.0.2",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "webpack": "^4.41.5",
